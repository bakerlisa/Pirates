\n        setNewCount({\n            [itemToCount]:  counting\n        })\n\n        setPirate({\n            ...pirate,\n            [itemToCount]: counting\n        })\n\n        axios.patch(`http://localhost:8000/api/edit/pirate/${id}`,newCount).then(response=>{\n            console.log(response.data)\n        })\n    }\n\n    return(\n        <div className={styled.singlePirate}>\n            <h1> { pirate.crew === \"Captain\" ?    \"Captain\" : \"\" } {pirate.name} </h1>\n            <h3>\"{pirate.phrase}\"</h3>\n            <img src={pirate.image} alt=\"\" />\n            <div className=\"ship\">{ pirate.crew} of the {pirate.ship} </div>\n            <p><strong>You'll know him by: </strong> \n                {\n                    attr.map((item,i) => {return <span key={i}>{item}, </span>\n                    })\n                }\n            </p>\n            <p className='ship'></p>\n            <p><strong>Treaseure: </strong>{pirate.treasure}</p>\n            <p><strong>He's Recieved: </strong> {pirate.likes} five star reviews</p>\n\n            <div className={styled.halloweenVote}>\n                <h2>Halloweed Dressup Vote:</h2>\n                <p>For halloween {pirate.name} should wear (click to vote): </p>\n\n                <p className={styled.vote} onClick={() => coutUp(\"eyePatch\",pirate.eyePatch) }>Eye Patch: <strong>{pirate.eyePatch}</strong></p>\n                <p className={styled.vote}  onClick={() => coutUp(\"pegLeg\",pirate.pegLeg) }>Peg Leg: <strong>{pirate.pegLeg}</strong></p>\n                <p className={styled.vote}  onClick={() => coutUp(\"hook\",pirate.hook) }>Hook: <strong>{pirate.hook}</strong></p>\n            </div>\n\n            <h3>Q&A:</h3>\n            <p>Does he have an eye patch: {pirate.featurePatch === true ? \"Yes!\" : \"No\"} </p>\n            <p>Does he have a peg leg: {pirate.featurePegleg === true ? \"Yes!\" : \"No\"} </p>\n            <p>Does he have a hook: {pirate.featureHook === true ? \"Yes!\" : \"No\"} </p>\n\n            <Link className=\"edit\" to={`/editPirate/${id}`}>Edit</Link>\n            <Delete id={id} />\n        </div>\n    )\n}\n\nexport default SinglePirate;","/Users/lisabroadhead/Desktop/Pirates/client/src/views/Login.js",[],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/Form.js",[],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/Pirate.js",["105","106"],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/Delete.js",["107"],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/WelcomeForm.js",["108","109","110"],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/RegistrationForm.js",["111","112"],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/Like.js",["113"],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/Banner.js",[],[],"/Users/lisabroadhead/Desktop/Pirates/client/src/components/Navigation.js",[],[],{"ruleId":"114","severity":1,"message":"115","line":1,"column":23,"nodeType":"116","messageId":"117","endLine":1,"endColumn":27},{"ruleId":"114","severity":1,"message":"118","line":1,"column":38,"nodeType":"116","messageId":"117","endLine":1,"endColumn":48},{"ruleId":"114","severity":1,"message":"119","line":4,"column":8,"nodeType":"116","messageId":"117","endLine":4,"endColumn":14},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","severity":1,"message":"125","line":28,"column":8,"nodeType":"126","endLine":28,"endColumn":12,"suggestions":"127"},{"ruleId":"114","severity":1,"message":"128","line":1,"column":26,"nodeType":"116","messageId":"117","endLine":1,"endColumn":35},{"ruleId":"120","replacedBy":"129"},{"ruleId":"122","replacedBy":"130"},{"ruleId":"114","severity":1,"message":"115","line":4,"column":10,"nodeType":"116","messageId":"117","endLine":4,"endColumn":14},{"ruleId":"124","severity":1,"message":"131","line":18,"column":8,"nodeType":"126","endLine":18,"endColumn":10,"suggestions":"132"},{"ruleId":"114","severity"